<h3>Nieruchomości</h3>

<div id="assets_first" class="box">

    <% if(properties.length) { %>

        <table>
            <tbody>
                <tr>
                    <th>Typ</th>
                    <th>Adres</th>
                    <th>Lista wpływów (brutto)</th>
                </tr>

        <% properties.forEach(property => { %>
            <tr>
                <td><%= property.type %></td>
                <td><%= property.city %> <%= property.postcode %> <%= property.street %>/<%= property.apartment_nr %></td>
                <td><a href="/dashboard/asset/<%= property._id %>/incomes"><button class="btn">Wpływy</button></a></td>
            </tr>
        <% }) %>

            </tbody>
        </table>

    <% } %>
</div>