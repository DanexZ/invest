        <div class="pools-list" >

            <div class="pools-list-header">
                <div>Okres</div>
            </div>

        <%   billingPeriods.forEach(billingPeriod => { %>

                <a href="/dashboard/money/billingPeriod/<%= billingPeriod.period.startingDate %>">
